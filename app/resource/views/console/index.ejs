<div class="row">
    <div class="col-xs-6">
        <div class="panel panel-default">
            <div class="panel-body">
                <form action="/query/run" method="post" data-target="console_01_output">
                    <ul class="list-unstyled">
                        <li class="form-group">
                            <div class="row">
                                <ul class="list-unstyled">
                                    <li class="col-xs-1">
                                        <h2>#01</h2>
                                    </li>
                                    <li class="col-xs-4">
                                        <select name="database" class="form-control" required>
                                            <option value="">Selecione o banco de dados</option>
                                        </select>
                                    </li>
                                    <li class="col-xs-7">
                                        <button type="submit" class="btn btn-default btn-success">
                                        <span class="glyphicon glyphicon-ok"></span>
                                         &nbsp; Executar
                                        </button>

                                        <a href="" class="btn-query-load btn btn-primary" data-target="console_01">
                                        <span class="glyphicon glyphicon-open-file"></span>
                                         &nbsp; Carregar
                                        </a>

                                        <a href="" class="btn-query-save btn btn-primary" data-save="console_01" data-target="console_01_output">
                                        <span class="glyphicon glyphicon-save-file"></span>
                                         &nbsp; Salvar
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="form-group">
                            <label class="sr-only" for="console_01">Console</label>
                            <textarea name="query" class="console form-control" id="console_01" required></textarea>
                        </li>
                    </ul>
                </form>
                <div class="output table-responsive" id="console_01_output">
                    <table class="table table-striped table-bordered table-hover table-condensed">
                        <thead></thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
